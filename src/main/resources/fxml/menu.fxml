<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="EMS.GUI.MenuPresenter" alignment="center" spacing="10" prefWidth="400" prefHeight="200">
    <Label text="Number of Elevators:"/>
    <Spinner fx:id="Elevators" min="1" max="16" initialValue="1"/>
    <Label text="Number of Floors:"/>
    <Spinner fx:id="Floors" min="2" max="10" initialValue="2"/>
    <Button text="Start" onAction="#Start"/>
</VBox>